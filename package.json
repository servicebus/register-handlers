{
  "name": "@servicebus/register-handlers",
  "version": "0.0.0-development",
  "description": "module for registering all servicebus handlers in a folder, according to convention",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/mocha test -R spec --recursive",
    "format": "prettier-standard 'test/**/*.js' 'test-mjs/**/*.mjs' 'index.js'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mateodelnorte/servicebus-register-handlers.git"
  },
  "lint-staged": {
    "linters": {
      "**/*.js": [
        "prettier-standard",
        "git add"
      ],
      "**/*.mjs": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "keywords": [
    "servicebus",
    "bus",
    "handlers"
  ],
  "author": "mateodelnorte",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mateodelnorte/servicebus-register-handlers/issues"
  },
  "homepage": "https://github.com/mateodelnorte/servicebus-register-handlers",
  "dependencies": {
    "amqp-match": "0.0.0",
    "async": "^2.0.0",
    "debug": "^4.0.0",
    "objectify-folder": "^1.1.4"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "jest": "^24.7.0",
    "lint-staged": "^8.1.5",
    "mocha": "6.0.0",
    "prettier-standard": "^9.1.1",
    "should": "13.2.3",
    "sinon": "7.2.2"
  }
}
